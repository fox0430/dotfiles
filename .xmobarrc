Config { font = "xft:Ricty-10:bold"
	, bgColor = "black"
	, fgColor = "grey"
	, position = Top
	, lowerOnStart = False
  , alpha = 150
	, commands = [ Run Weather "RJTT" ["-t","<station>: <tempC>C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
				, Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
				, Run Network "wlp3s0" ["-L","0","-H","32","--normal","green","--high","red"] 10
				, Run Battery [] 10
				, Run Memory ["-t","Mem: <usedratio>%"] 10
				, Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
				, Run Com "bash" ["/home/fox/.xmonad/get-volume.sh"] "myvolume" 10
				, Run StdinReader
	]
	, sepChar = "%"
	, alignSep = "}{"
	,template = "%cpu% | %memory% | %wlp3s0% | %myvolume% | %battery%} %StdinReader% { %RJTT% | <fc=#ee9a00>%date%</fc>"
}
